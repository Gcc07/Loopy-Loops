
public class NumberStairs {

    public static void main(String[] args) {
        String line_print = "";

        for (int i = 0; i <= 11; i++) { // Go through 10 iterations
            for (int n = 1; n < i; n++) {
                line_print += n;
            }
            System.out.println(line_print);
            line_print = "";
        
        }
    }
}
